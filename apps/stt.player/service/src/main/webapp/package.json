{
   "name": "stt.player",
   "version": "0.0.1",
   "description": "Experimental Apollo GraphQL Backend",
   "main": "dist/out-tsc/src/server.js",
   "babel": {
      "presets": [
         "env"
      ]
   },
   "scripts": {
      "test": "jasmine",
      "prebuild": "apollo-codegen introspect-schema --output ../../../target/graphql-dependencies/schema.json ../../../target/graphql-dependencies/stt/player/schema.graphql",
      "build:cev": "cev --prefix STT > config/custom-environment-variables.json",
      "build": "tsc -p tsconfig.json",
      "start": "ts-node src/index.ts",
      "unused:download": "apollo-codegen download-schema http://localhost:27474/graphql/ --output schema.json -H 'Authorization: Basic bmVvNGo6cG9ydGZvbGlv'",
      "unused:generate": "apollo-codegen generate src/graphql/*.graphql --schema ../../../target/graphql-dependencies/schema.json --target typescript --output src/graphql/schema.ts"
   },
   "author": "John Heinnickel",
   "license": "ISC",
   "dependencies": {
      "@jchein/bmodular": "^0.0.1-1",
      "apollo-engine": "^0.8.1",
      "apollo-link": "^1.0.0",
      "apollo-server-hapi": "^1.1.7",
      "body-parser": "^1.18.2",
      "casual": "^1.5.19",
      "compression": "^1.7.1",
      "config": "^1.28.1",
      "fluent-interface-builder": "^2.0.1",
      "graphql": "^0.11.7",
      "hapi": "^17.2.0",
      "hydrate": "^1.0.0",
      "inversify": "^4.9.0",
      "inversify-binding-decorators": "^3.2.0",
      "inversify-devtools": "^1.0.0",
      "inversify-vanillajs-helpers": "^2.0.0",
      "js-yaml": "^3.10.0",
      "mongoose": "^4.13.7",
      "mysql": "^2.14.1",
      "neo4j": "^2.0.0-RC2",
      "neo4j-graphql-js": "^0.1.5",
      "node-fetch": "^1.7.3",
      "reflect-metadata": "^0.1.10",
      "sequelize": "^4.28.6",
      "sqlite3": "^3.1.13",
      "subscriptions-transport-ws": "^0.9.3",
      "superagent": "^3.8.2",
      "tslib": "^1.8.0",
      "typeorm": "0.1.9"
   },
   "devDependencies": {
      "@types/graphql": "^0.11.5",
      "@types/hapi": "^16.1.12-1",
      "@types/node": "^8.0.29",
      "@types/zen-observable": "^0.5.3",
      "apollo-codegen": "^0.18.3",
      "babel-cli": "^6.26.0",
      "babel-preset-env": "^1.6.1",
      "config-cev-generator": "^0.1.6",
      "cross-env": "^5.1.3",
      "eslint": "^4.14.0",
      "eslint-plugin-hapi": "^4.1.0",
      "hapi-auth-jwt2": "^7.3.0",
      "hapi-cors-headers": "^1.0.3",
      "joi": "^13.0.2",
      "nodemon": "^1.14.6",
      "ts-node": "3.3.0",
      "tslint": "^5.8.0",
      "typescript": "2.4.2"
   }
}
