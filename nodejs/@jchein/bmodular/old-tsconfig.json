{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "noEmit": false,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitUseStrict": false,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noEmitOnError": false,
    "lib": [
      "ES2015", "ES2015.Symbol", "ES2015.Iterable", "DOM"
    ]
  },
  "include": [
    "scripts/**/*.ts"
  ],
  "exclude": [ ],
  "compileOnSave": true,
  "buildOnSave": true
}
