{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../distTest",
    "noEmit": false,
    "allowJs": false,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitUseStrict": false,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "noEmitOnError": false,
    "typeRoots": [ "../node_modules/@types" ],
    "lib": [
      "ES6", "ES2015.Symbol", "ES2015.Iterable", "DOM"
    ]
  },
  "filesGlob": [
    "../spec/**/*.ts"
  ],
  "include": [
    "../lib/**/*.ts",
    "../spec/**/*.ts"
  ],
  "exclude": [
    "../node_modules/**/*.ts"
  ],
  "compileOnSave": true,
  "buildOnSave": true
}
